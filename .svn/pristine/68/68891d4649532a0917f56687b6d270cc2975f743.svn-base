                            .Build();

            soccerLinejob = JobBuilder.Create<Crawlnate>()
                            .WithIdentity("soccerLinejob", "group1")
                            .Build();

            ITrigger trigger = TriggerBuilder.Create()
                   .WithIdentity("trigger1", "group1")
                   .StartNow()
                   .WithSimpleSchedule(x => x
                       .WithIntervalInSeconds(10)
                       .RepeatForever())
                   .Build();

